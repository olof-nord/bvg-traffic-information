{
  "name": "traffic-information-app",
  "description": "This project uses the BVG Verkehrsmeldungen API and visualises the status information",
  "version": "0.4.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:olof-nord/bvg-traffic-information.git"
  },
  "author": {
    "name": "Olof Nord"
  },
  "keywords": [
    "public-transport",
    "transit",
    "bvg",
    "berlin",
    "germany"
  ],
  "scripts": {
    "clean": "rimraf api/* src/environments dist/*",
    "generate-env": "ts-node etc/generate-env.ts",
    "prestart": "npm run clean && npm run generate-env dev",
    "start": "ng-openapi-gen && ng serve",
    "prebuild": "npm run clean && npm run generate-env prod",
    "build": "ng-openapi-gen && ng build --prod",
    "test": "ng test",
    "test:dev": "ng test --watch=true",
    "lint": "ng lint",
    "pree2e": "node_modules/protractor/bin/webdriver-manager update --versions.chrome=80.0.3987.106 --gecko false --standalone false",
    "e2e": "ng e2e",
    "deploy": "ng deploy"
  },
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/localize": "^9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^6.0.3",
    "@ngrx/effects": "^9.1.0",
    "@ngrx/store": "^9.1.0",
    "@ngrx/store-devtools": "^9.1.0",
    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@ngrx/schematics": "^9.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-openapi-gen": "^0.10.0",
    "protractor": "~5.4.3",
    "puppeteer": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
